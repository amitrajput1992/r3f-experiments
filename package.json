{
  "name": "r3f-experiments",
  "version": "1.0.0",
  "main": "./lib/cjs/src/index.js",
  "module": "./lib/esm/src/index.js",
  "repository": "https://github.com/amitrajput1992/r3f-experiments",
  "author": "Amit rajput",
  "license": "UNLICENSED",
  "sideEffects": false,
  "files": [
    "lib/*"
  ],
  "scripts": {
    "@comment RUN": "Running this repo after running this once - pnpm install",
    "watch": "$(pnpm bin)/start-storybook -p 8081 --ci",
    "check": "$(pnpm bin)/tsc --noEmit && $(pnpm bin)/webpack build --config dev/webpack/webpack.prod.config.js;",
    "lint": "$(pnpm bin)/eslint --ext \".ts,.tsx,.js,.jsx\" ./src --fix",
    "buildesm": "$(pnpm bin)/tsc --project src/ -p src/tsconfig-esm.json; cpy '**/*' '!**/*.{ts,tsx}' ../lib/esm/src/ --cwd=src/ --no-overwrite --parents;",
    "buildcjs": "$(pnpm bin)/tsc --project src/ -p src/tsconfig-cjs.json; cpy '**/*' '!**/*.{ts,tsx}' ../lib/cjs/src/ --cwd=src/ --no-overwrite --parents;",
    "build": "del lib; pnpm buildesm && pnpm buildcjs",
    "buildstorybook": "rm -rf build; mkdir -p build; build-storybook -o build"
  },
